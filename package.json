{
  "name": "angular-schematics-webcomponents",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:clean": "node --experimental-modules ./sripts/run-local.mjs",
    "run:local": "npm run run:clean && npm run schematics:build && npm run schematics:link",
    "postrun:local": "ng g @wizco/schematics-webcomponents:init",
    "schematics:intall": "yarn --cwd ./application-webcomponents install",
    "schematics:build": "yarn --cwd ./application-webcomponents build",
    "schematics:link": "npm unlink ./application-webcomponents && npm link ./application-webcomponents",
    "extra:build": "ng build --project loklok --output-hashing none --aot --build-optimizer",
    "extra:build:staging": "ng build --configuration=staging --project loklok --output-hashing none --aot --build-optimizer",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^15.0.3",
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/elements": "^15.2.5",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@schematics/angular": "^15.2.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "updated": "^2.0.5",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ngx-build-plus": "^15.0.0",
    "typescript": "~4.9.4"
  }
}