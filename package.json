{
  "name": "angular-schematics-webcomponents",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:clean": "node --experimental-modules ./sripts/run-local.mjs",
    "run:local": "npm run run:clean && npm run schematics:build && npm run schematics:link",
    "postrun:local": "ng g @wizco/schematics-webcomponents:init",
    "schematics:intall": "yarn --cwd ./application-webcomponents install",
    "schematics:build": "yarn --cwd ./application-webcomponents build",
    "schematics:link": "npm unlink ./application-webcomponents && npm link ./application-webcomponents",
    "wiztools:publish": "ng build wizpro-tools && npm publish ./dist/wizpro-tools",
    "start:module": "ng serve --project botafogo",
    "extra:build": "ng build --project botafogo --output-hashing none --aot --build-optimizer",
    "extra:build:staging": "ng build --configuration=staging --project botafogo --output-hashing none --aot --build-optimizer",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^17.0.1",
    "@angular/animations": "^17.1.2",
    "@angular/cdk": "^17.2.0",
    "@angular/common": "^17.1.2",
    "@angular/compiler": "^17.1.2",
    "@angular/core": "^17.1.2",
    "@angular/elements": "^17.1.2",
    "@angular/forms": "^17.1.2",
    "@angular/platform-browser": "^17.1.2",
    "@angular/platform-browser-dynamic": "^17.1.2",
    "@angular/router": "^17.1.2",
    "@schematics/angular": "^16.2.7",
    "@wizco/fenixds-core": "^1.0.22",
    "@wizco/fenixds-ngx": "^17.0.19",
    "@wizco/wizpro-tools": "^17.0.10",
    "jwt-decode": "^3.1.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.1.2",
    "@angular/compiler-cli": "^17.1.2",
    "@types/jasmine": "~5.1.1",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.1.0",
    "ngx-build-plus": "^17.0.0",
    "typescript": "~5.3.3"
  }
}