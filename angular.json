{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-schematics-webcomponents": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-schematics-webcomponents",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "angular-schematics-webcomponents:build:production"
            },
            "development": {
              "buildTarget": "angular-schematics-webcomponents:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "wizpro-tools": {
      "projectType": "library",
      "root": "libs/wizpro-tools",
      "sourceRoot": "libs/wizpro-tools/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "libs/wizpro-tools/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/wizpro-tools/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/wizpro-tools/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "libs/wizpro-tools/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "loadl": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/loadl",
      "sourceRoot": "projects/loadl/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/loadl",
            "index": "projects/loadl/src/index.html",
            "main": "projects/loadl/src/main.ts",
            "polyfills": "projects/loadl/src/polyfills.ts",
            "tsConfig": "projects/loadl/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/loadl/src/favicon.ico",
              "projects/loadl/src/assets"
            ],
            "styles": [
              "projects/loadl/src/styles.scss"
            ],
            "scripts": [],
            "extraWebpackConfig": "projects/loadl/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/loadl/src/environments/environment.ts",
                  "with": "projects/loadl/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "projects/loadl/webpack.prod.config.js"
            },
            "staging": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/loadl/src/environments/environment.ts",
                  "with": "projects/loadl/src/environments/environment.staging.ts"
                }
              ],
              "outputHashing": "all"
            },
            "sandbox": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/loadl/src/environments/environment.ts",
                  "with": "projects/loadl/src/environments/environment.sandbox.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "b2c": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/loadl/src/environments/environment.ts",
                  "with": "projects/loadl/src/environments/environment.b2c.ts"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "port": 4430,
            "publicHost": "http://localhost:4430",
            "extraWebpackConfig": "projects/loadl/webpack.config.js"
          },
          "configurations": {
            "production": {
              "buildTarget": "loadl:build:production",
              "extraWebpackConfig": "projects/loadl/webpack.prod.config.js"
            },
            "development": {
              "buildTarget": "loadl:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/loadl/src/**/*.ts",
              "projects/loadl/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "projects/loadl/src/test.ts",
            "polyfills": "projects/loadl/src/polyfills.ts",
            "tsConfig": "projects/loadl/tsconfig.spec.json",
            "karmaConfig": "projects/loadl/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/loadl/src/favicon.ico",
              "projects/loadl/src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}